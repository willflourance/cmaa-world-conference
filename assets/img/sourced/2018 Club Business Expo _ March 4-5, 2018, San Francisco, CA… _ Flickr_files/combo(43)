YUI.add("photo-writeability-models",function(e,t){function r(e){r.superclass.constructor.call(this,e)}e.Models[this.name]=r,e.extend(r,e.FlickrModelRegistry,{name:this.name,remote:{read:function(t){return e.ModelFetchers["flickr-photos-getWriteability"].run(t,this.appContext).then(function(e){return e})}},attributes:{writeable:{validator:function(t,r){return e.AttributeHelpers.validateBoolean(t)},setter:function(t){return e.AttributeHelpers.coerceBoolean(t)||!1},defaultValue:!1}}})},"@VERSION@",{requires:["flickr-model-registry","flickr-promise","flickr-photos-getWriteability-fetcher","attribute-helpers"]});YUI.add("flickr-photos-getWriteability-fetcher",function(e,t){"use strict";e.namespace("ModelFetchers")["flickr-photos-getWriteability"]={run:function(i,r){var o=this;return e.Promise.all([r.callAPI("flickr.photos.getWriteability",{photo_id:i.id}),r.getModelRegistry("photo-writeability-models")]).then(function(e){return o._processResponse(e,i)},e.FetcherErrorLogger(t))},_processResponse:function(e,t){var i=e[0],r=e[1];if(i&&"ok"===i.stat)return r.addOrUpdate({id:t.id,writeable:i.writeability})}}},"@VERSION@",{requires:["flickr-promise","api-helper"],optional:["photo-writeability-models"]});